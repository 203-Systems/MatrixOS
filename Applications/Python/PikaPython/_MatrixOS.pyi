from MatrixOSColor import Color
from MatrixOSPoint import Point

# System functionsdef Reboot(self) -> None: ...
def Bootloader() -> None: ...
def DelayMs(ms: int) -> None: ...
def Millis() -> int: ...
def OpenSetting() -> None: ...
def ExecuteAPP(author: str, app_name: str) -> None: ...
def ExecuteAPPByID(app_id: int) -> None: ...
