class LED:
    def NextBrightness(self) -> None: ...
    def SetBrightness(self, brightness: int) -> None: ...
    def SetBrightnessMultiplier(self, partition_name: str, multiplier: float) -> None: ...
    def SetColor(self, xy: Point, color: Color, layer) -> None: ...
    def SetColorByID(self, id: int, color: Color, layer) -> None: ...
    def Fill(self, color: Color, layer) -> None: ...
    def FillPartition(self, partition: str, color: Color, layer) -> None: ...
    def Update(self, layer) -> None: ...
    def CurrentLayer(self) -> int: ...
    def CreateLayer(self, crossfade) -> int: ...
    def CopyLayer(self, dest: int, src: int) -> None: ...
    def DestroyLayer(self, crossfade) -> bool: ...
    def Fade(self, crossfade) -> None: ...
    def PauseUpdate(self, pause: bool = True) -> None: ...
    def GetLedCount(self) -> int: ...

class KeyPad:
    def Get(self, timeout_ms: int) -> KeyEvent: ...
    def GetKey(self, keyXY: Point) -> KeyInfo: ...
    def GetKeyByID(self, keyID: int) -> KeyInfo: ...
    def Clear(self) -> None: ...
    def XY2ID(self, xy: Point) -> int: ...
    def ID2XY(self, keyID: int) -> Point: ...

